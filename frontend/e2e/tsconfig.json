/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/e2e",
    "module": "commonjs",
    "target": "es2018",
    "fullTemplateTypeCheck": true, 
    "strictInjectionParameters": true, 
    "files": ["src/main.ts", "src/polyfills.ts"],
    "include": ["src/**/*.d.ts"],
    "enableIvy": false,
    "types": [
      "jasmine",
      "jasminewd2",
      "node"
    ]
  }
}
